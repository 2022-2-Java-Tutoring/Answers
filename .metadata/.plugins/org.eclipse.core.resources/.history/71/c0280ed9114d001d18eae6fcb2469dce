import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("2자리수 정수 입력(10~99)>>" );
		int num = scanner.nextInt();
		scanner.close();
		
		int a = num / 10;
		int b = num % 10;
		
		System.out.printf(
				"%s 10의 자리와 1의 자리가 %s.\n",
				(a==b) ? "Yes!": "No!",
				(a==b) ? "같습니다": "다릅니다");
	}
}